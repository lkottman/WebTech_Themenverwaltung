        <div class="dropdown">
            <form action="/userInfo">
                <button class="header_button">Benutzer</button>
            </form>
        </div>
        <div class="dropdown">
            <form action="/logout">
                <button class="header_button">Logout</button>
            </form>
        </div>
    </div>
</div>


<!-- Anzeige des Moduls, Themas, Datum und Raum der Pr채sentation. Wird aus der Datenbank entnommen-->
<h1>Modul: <%= modulname %></h1>
<h2>Anlass: <%= anlass %></h2>
<h2>Datum: <%= tag %></h2>
<h2>Raum: <%= raum %></h2>
<table rules="rows">
    <thead>
    <tr>
        <th>&nbsp</th>
        <th>Reihenfolge</th>
        <th>Gruppe</th>
        <th>Thema</th>
        <th>Anzahl der Gruppenteilnehmer</th>
        <th>geplante Zeit</th>
        <th>geplante Dauer</th>
        <th>geplantes Ende</th>
        <th>Raum</th>
    </tr>
    </thead>
    <tbody>
    <!-- Hier werden die einzelnen Tabellenzeilen geladen, die Anzahl der Zeilen ist abh채ngig von den Eintr채gen der Datenbank-->
    <% for (var i = 0; i < reihenfolge.length;i++) { %>
        <tr>
            <td>&nbsp;</td>
            <td><%= reihenfolge[i] %></td>
            <td><%= gruppe[i] %></td>
            <td><%= thema[i] %></td>
            <td><%= mitglieder[i] %></td>
            <td><%= startzeit[i] %></td>
            <td><%= dauer[i] %></td>
            <td><%= ende[i] %></td>
            <td><%= raum %></td>
        </tr>
    </tbody>
    <% } %>
</table>
</form>
</body>
</html>